        <dcterms:identifier xml:lang="en-us">{"id": "98ef35ad-d178-4ad3-93e7-86dcd3a3124b", "value": "http://www.archesproject.org/c812c32b-7adc-4c37-9c6c-9436fa63eaeb"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/6f9b6a55-da93-45ad-abad-300e452694f7">
        <dcterms:identifier xml:lang="en-us">{"id": "3d57fdc0-f509-4013-8ea8-c32ecab9f768", "value": "http://www.archesproject.org/6f9b6a55-da93-45ad-abad-300e452694f7"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
        <skos:scopeNote xml:lang="en">{"id": "3cea3ae1-3051-422e-8dd2-2ce13337eb49", "value": "The second subdivision of the Palaeolithic or Old Stone Age. Characterized by the fine flake tools of the Mousterian tradition and economically by a hunter gatherer society.\n"}</skos:scopeNote>
        <skos:prefLabel xml:lang="en">{"id": "ac8bd142-94ec-4191-a921-9bff49eaf983", "value": "MIDDLE PALAEOLITHIC"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
    <skos:prefLabel xml:lang="en">{"id": "7eaff630-76ae-44ea-95e3-d9339e1d1ea4", "value": "PALAEOLITHIC"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en-us">{"id": "7d3f8978-eae3-4f94-9024-9ab9325e74d0", "value": "http://www.archesproject.org/c5382b46-7049-4e4a-a51b-d79db123012f"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "509bb151-b3b7-4132-8ec9-0a8669c29cca", "value": "The period once referred to as the Old Stone Age. It is defined by the practice of hunting and gathering and the use of chipped flint tools. This period is usually divided up into the Lower, Middle and Upper Palaeolithic."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/5f654c6b-d96a-455e-a9d2-85c113deedaa">
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:scopeNote xml:lang="en">{"id": "060f9aae-910e-4cd3-aba8-58d35453d70c", "value": "Begins with the dissolution of the monasteries and ends with the death of Queen Victoria. Use more specific period where known.\n"}</skos:scopeNote>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/e972997b-1ed0-4b3f-af4c-7f083307ed57">
        <skos:prefLabel xml:lang="en">{"id": "47b465b5-f951-4da0-bf18-4afaa9e0a4b0", "value": "HANOVERIAN"}</skos:prefLabel>
        <skos:narrower rdf:resource="http://localhost:8000/990ce1c2-fadb-48d3-a534-256f3b6194bf"/>
        <skos:narrower rdf:resource="http://localhost:8000/9032ba54-5b96-4dde-9f60-f6dd967e8e4b"/>
        <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
        <skos:scopeNote xml:lang="en">{"id": "a580741a-4494-4c9f-b999-62427bd47c96", "value": "Dating to the reign of the Hanoverian kings and queens.    "}</skos:scopeNote>
        <dcterms:identifier xml:lang="en-us">{"id": "d1468e56-b7ce-4f6b-a5dc-18d5aa93e5c5", "value": "http://www.archesproject.org/e972997b-1ed0-4b3f-af4c-7f083307ed57"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
    <dcterms:identifier xml:lang="en-us">{"id": "de0deae7-0b1d-4396-8e18-1d7a1efae513", "value": "http://www.archesproject.org/5f654c6b-d96a-455e-a9d2-85c113deedaa"}</dcterms:identifier>
    <skos:narrower rdf:resource="http://localhost:8000/6bae40ea-bfa7-414f-8a42-90e8c18f613c"/>
    <skos:prefLabel xml:lang="en">{"id": "c07a7488-4890-4645-a323-4992a2bba7f5", "value": "POST MEDIEVAL"}</skos:prefLabel>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/7b4c5c00-8652-4029-910b-1769fa6b7b01">
        <skos:scopeNote xml:lang="en">{"id": "c5c591c4-bab4-4a05-9fd5-be51d3e775b0", "value": "Dating to the reign of the Stuart kings of England (including the Commonwealth inter-regnum)      "}</skos:scopeNote>
        <skos:narrower rdf:resource="http://localhost:8000/ef2ae957-72d8-4ceb-922e-2c463ff49aa1"/>
        <dcterms:identifier xml:lang="en-us">{"id": "8f5a0a1f-0dbd-4fa2-8d2b-fbffecbdd991", "value": "http://www.archesproject.org/7b4c5c00-8652-4029-910b-1769fa6b7b01"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "fcc9d3bf-355d-4664-8eaf-64c0296512b6", "value": "STUART"}</skos:prefLabel>
        <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
      </skos:Concept>
    </skos:narrower>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/245a684b-2198-48d3-a620-f843f757af4a">
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:prefLabel xml:lang="en">{"id": "596fb053-28bc-4d43-ad1e-b425fc3522f1", "value": "EARLY MEDIEVAL"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "e92c8915-a7aa-4295-adf3-3c6ad2b8bb07", "value": "This dates from the breakdown of Roman rule in Britain to the Norman invasion in 1066 and is to be used for monuments of post Roman, Saxon and Viking date.\n"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "5f00300e-00f4-4e7d-a46a-54274afe244f", "value": "http://www.archesproject.org/245a684b-2198-48d3-a620-f843f757af4a"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/fde52573-63e6-457a-90ea-42d8e61d4a7b">
    <skos:scopeNote xml:lang="en">{"id": "340bb953-4c56-428f-a96d-5a118daedd1f", "value": "The Medieval period or Middle Ages begins with the Norman invasion and ends with the dissolution of the monasteries.\n"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "e96a7115-09c0-4b76-a33b-60a3a8acbb0c", "value": "MEDIEVAL"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:narrower rdf:resource="http://localhost:8000/6bae40ea-bfa7-414f-8a42-90e8c18f613c"/>
    <dcterms:identifier xml:lang="en-us">{"id": "13c711a4-fc7d-4d7d-b0c4-0b5f38883375", "value": "http://www.archesproject.org/fde52573-63e6-457a-90ea-42d8e61d4a7b"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1ae36cab-b58b-4244-a21d-426dfafefe10">
