        <skos:scopeNote xml:lang="en">{"id": "417225a6-aa9b-45de-89f4-424582c77699", "value": "The second subdivision of the Iron Age.\n"}</skos:scopeNote>
        <dcterms:identifier xml:lang="en-us">{"id": "95622acd-9ae3-40bb-93fc-f7276dc491ad", "value": "http://www.archesproject.org/15a021f9-000e-4fd4-b92d-6872dbbdc56b"}</dcterms:identifier>
