    <dcterms:identifier xml:lang="en-us">{"id": "62cd8bd5-fd98-4bfa-8580-e9f1f5b53b75", "value": "http://www.archesproject.org/1ae36cab-b58b-4244-a21d-426dfafefe10"}</dcterms:identifier>
    <skos:narrower rdf:resource="http://localhost:8000/b8da8b77-71d3-4f62-bafb-e8e14b5290d5"/>
    <skos:narrower rdf:resource="http://localhost:8000/05813b48-b4c3-4b81-a7b4-b27e9cb73d0f"/>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:prefLabel xml:lang="en">{"id": "a55ceb8b-e8d4-4eb2-be78-fab2df7e278c", "value": "20TH CENTURY"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "17226952-1f62-4878-8897-98be9e2e1bf7", "value": "Previously recorded as 'Modern'. Covers the period from 1901-2000.     "}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/cf5561f8-b02a-46e0-831b-b9d1904a3862">
