    <dcterms:identifier xml:lang="en-us">{"id": "fc8f9a4a-4fc0-43a1-910f-66733f63a02f", "value": "http://www.archesproject.org/ea1dde12-1557-4411-8ec4-e3fd569f9303"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "8c99b579-d9b4-4c40-bb34-de4e47575cbe", "value": "EARLY BRONZE AGE"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "7b0c733e-cacf-4e2c-91e1-f243c342b161", "value": "The earliest subdivision of the Bronze Age.\n"}</skos:scopeNote>
