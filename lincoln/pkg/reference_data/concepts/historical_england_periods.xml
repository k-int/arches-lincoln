  <skos:Concept rdf:about="http://localhost:8000/9032ba54-5b96-4dde-9f60-f6dd967e8e4b">
    <skos:prefLabel xml:lang="en">{"id": "3b7359ca-8b38-44f6-ac7f-bc9a1e73fa0f", "value": "VICTORIAN"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "a365fe4d-0031-4402-9eb4-5408b983a26d", "value": "Dating to the reign of Queen Victoria."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "93fdb792-4cff-4d33-9f5a-5b2ff8197bbb", "value": "http://www.archesproject.org/9032ba54-5b96-4dde-9f60-f6dd967e8e4b"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/990ce1c2-fadb-48d3-a534-256f3b6194bf">
    <skos:prefLabel xml:lang="en">{"id": "45c22ff1-27fd-4755-9ef2-d852d9d514d5", "value": "GEORGIAN"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "568451cf-4202-4baf-988d-12d5569c39ca", "value": "Dating to or characteristic of the reigns of any of the first four kings of Great Britain called George.\n"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <dcterms:identifier xml:lang="en-us">{"id": "d6821b7c-9a28-426d-8900-7a0e914f1bd3", "value": "http://www.archesproject.org/990ce1c2-fadb-48d3-a534-256f3b6194bf"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/c43fbe87-3763-47ca-a336-38f2eca94b22">
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:scopeNote xml:lang="en">{"id": "522cd6b2-24cd-442b-b1f9-101bea850864", "value": "The Middle Stone Age, falling between the Palaeolithic and the Neolithic; marks the beginning of a move from a hunter gatherer society towards food producing society.\n"}</skos:scopeNote>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/c5e4ae8d-9478-45e6-b934-c1fb3ab33d8f">
        <skos:scopeNote xml:lang="en">{"id": "1f4bde89-b8cd-417d-b9ef-42b37f8368b3", "value": "The earliest subdivision of the Mesolithic, or Middle Stone Age.\n"}</skos:scopeNote>
        <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
        <skos:prefLabel xml:lang="en">{"id": "70491c79-6722-41f8-8c54-7373dd15ff33", "value": "EARLY MESOLITHIC"}</skos:prefLabel>
        <dcterms:identifier xml:lang="en-us">{"id": "e132d115-7043-4a9b-b980-49e91aff1ac9", "value": "http://www.archesproject.org/c5e4ae8d-9478-45e6-b934-c1fb3ab33d8f"}</dcterms:identifier>
      </skos:Concept>
    </skos:narrower>
