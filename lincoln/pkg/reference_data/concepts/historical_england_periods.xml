    <skos:prefLabel xml:lang="en">{"id": "7961fb98-44f1-4942-8e59-4c60a8c555e8", "value": "21ST CENTURY"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "0cbe4817-97f9-4685-8e1d-d62de2ca9ecf", "value": "Dating to the start of the third millennium CE/AD.  2001-2100"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/22769b72-19e7-4249-a5a5-c7079b3796d6">
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:scopeNote xml:lang="en">{"id": "dc925585-638b-4058-bb94-057835a581ea", "value": "No longer used. Traditionally use for resources that could only be identified to a date range from Neolithic to Iron Age. Included here to aid migration.\n"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "2abe941f-2d21-412f-ba50-19c63bc11ea0", "value": "http://www.archesproject.org/22769b72-19e7-4249-a5a5-c7079b3796d6"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "64fe1470-1a60-491c-b462-04068a197b23", "value": "LATER PREHISTORIC"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/adf6690a-fbf0-41f9-a177-dfacbd3eb058">
    <skos:scopeNote xml:lang="en">{"id": "02797b8e-9371-4038-b93f-03b8bde5669d", "value": "Traditionally begins with the Roman invasion in 43AD and ends with the emperor Honorius directing Britain to see to its own defence in 410AD.\n"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "68a8a2f7-c045-43e0-8783-74d584ecd123", "value": "ROMAN"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <dcterms:identifier xml:lang="en-us">{"id": "45c16662-cbef-4f6f-9b46-2f50ba83782f", "value": "http://www.archesproject.org/adf6690a-fbf0-41f9-a177-dfacbd3eb058"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/ddf9bada-c7ef-4c60-aaa7-a49f8b882b0e">
    <skos:scopeNote xml:lang="en">{"id": "a961005e-1f32-4f9c-880f-1cf09e76ea90", "value": "No longer used. Traditionally used for resources that could only be identified to a date range from the Palaeolithic to Iron Age. Included here to aid migration.\n"}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "2346f2c2-1f7a-4651-b175-a440eef444a9", "value": "http://www.archesproject.org/ddf9bada-c7ef-4c60-aaa7-a49f8b882b0e"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "38bb1036-4720-48fa-8a5e-9455e83776e0", "value": "PREHISTORIC"}</skos:prefLabel>
    <skos:narrower rdf:resource="http://localhost:8000/22769b72-19e7-4249-a5a5-c7079b3796d6"/>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
  </skos:Concept>
