    <dcterms:identifier xml:lang="en-us">{"id": "8dc41fa6-8d21-43aa-9015-fab225fcc7e7", "value": "http://www.archesproject.org/a3985777-43de-47b0-b070-83690a92f22c"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "d19b2249-0b43-4209-8a64-da7559c93dac", "value": "The second subdivision of the Bronze Age."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "6d7fb623-90b1-4a60-b994-f6d641937d0d", "value": "MIDDLE BRONZE AGE"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/6ba234ef-5948-43b8-b347-cfe779aeb79c">
    <skos:scopeNote xml:lang="en">{"id": "eae42e99-1663-4a8d-97cd-a31fbb16b515", "value": "This period follows on from the Bronze Age and is characterized by the use of iron for making tools and monuments such as hillforts and oppida. The Iron Age is taken to end with the Roman invasion.\n"}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
    <skos:narrower>
      <skos:Concept rdf:about="http://localhost:8000/8764dd25-6d36-4be4-ae24-40088236c0e9">
        <skos:inScheme rdf:resource="http://localhost:8000/a666eade-733b-4a3d-a8b7-3dc483ebaaef"/>
        <skos:scopeNote xml:lang="en">{"id": "8cf7a9c8-8f51-4a8b-a411-26ff2d955557", "value": "The earliest subdivision of the Iron Age.\n"}</skos:scopeNote>
        <dcterms:identifier xml:lang="en-us">{"id": "880e8c7a-9e70-4e18-9f37-42903490ff1e", "value": "http://www.archesproject.org/8764dd25-6d36-4be4-ae24-40088236c0e9"}</dcterms:identifier>
        <skos:prefLabel xml:lang="en">{"id": "406c878f-3c7c-4f92-bc65-299bbc77fb92", "value": "EARLY IRON AGE"}</skos:prefLabel>
      </skos:Concept>
    </skos:narrower>
